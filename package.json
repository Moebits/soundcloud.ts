{
    "name": "soundcloud.ts",
    "version": "0.4.9",
    "description": "Wrapper for the Soundcloud API with typings",
    "main": "dist/soundcloud.js",
    "types": "dist/soundcloud.d.ts",
    "scripts": {
        "start": "tsc && node dist/start.js",
        "docs": "typedoc --out docs ./",
        "build": "tsc",
        "build:check": "tsc --noEmit",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "test": "ts-mocha --timeout 30000 -p tsconfig.json test/**/*.spec.ts",
        "test:coverage": "nyc npm run test",
        "prettier": "prettier --write \"**/*.{ts,json,yml,yaml,md}\"",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Tenpi/soundcloud.ts"
    },
    "keywords": [
        "soundcloud",
        "typescript",
        "ts",
        "audio",
        "music"
    ],
    "author": "Tenpi (https://github.com/Tenpi)",
    "contributors": [
        "Skick (https://github.com/skick1234)"
    ],
    "license": "MIT",
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@tenpi/tslint": "^1.1.0",
        "@tenpi/typedoc": "0.0.3",
        "@types/chai": "^4.2.18",
        "@types/mocha": "^8.2.2",
        "@types/node": "^15.3.0",
        "chai": "^4.3.4",
        "dotenv": "^9.0.2",
        "eslint": "^8.38.0",
        "eslint-config-distube": "^1.6.4",
        "mocha": "^8.4.0",
        "nyc": "^15.1.0",
        "prettier": "^2.8.7",
        "ts-mocha": "^10.0.0",
        "typedoc": "^0.20.36",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "audioconcat": "^0.1.4",
        "undici": "^5.8.0"
    },
    "nyc": {
        "extends": "@istanbuljs/nyc-config-typescript",
        "check-coverage": true,
        "all": true,
        "include": [
            "**/!(*.test.*).ts"
        ],
        "exclude": [
            "dist/**/*.*",
            "test/**/*.*"
        ],
        "reporter": [
            "html",
            "lcov",
            "text",
            "text-summary"
        ],
        "report-dir": "coverage"
    }
}
